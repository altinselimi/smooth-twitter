{
    "name": "Smooth Twitter",
    "version": "1.0",
    "description": "Smooth scroll on twitter using keyboard!",
    "manifest_version": 2,
    "permissions": [
        "activeTab"
    ],
    "content_scripts": [{
        "matches": ["https://twitter.com/home"],
        "js": ["contentScript.js"],
        "css": ["contentScript.css"]
    }],
    "icons":
    {
        "128": "smooth-twitter.png"
    }
}
